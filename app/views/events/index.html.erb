<% if user_signed_in? %>
    <iframe src="https://www.google.com/calendar/embed?title=<%= current_user.email %>&amp;height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=<%= current_user.email %>&amp;color=%232952A3&amp;ctz=America%2FNew_York" style=" border-width:0 " width="800" height="600" frameborder="0" scrolling="no"></iframe><br>
    <%= link_to "New Event", new_event_path, class: 'button' %><br>
    <%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'button' %>
<% else %>
    <%= link_to "Sign-in with Google Authentication.", new_user_session_path, class: 'button'  %>
<% end %>


